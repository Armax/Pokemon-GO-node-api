{
  //http://jshint.com/docs/options/
  "bitwise": true,
  "camelcase": false,
  "curly": false,
  "eqeqeq": false,
  "forin": true,
  "immed": true,
  "latedef": false,
  "newcap": false,
  "noarg": false,
  "noempty": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "single",
  "regexp": true,
  "undef": false,
  "unused": false,
  "strict": false,
  "trailing": true,

  "indent": 2,
  "maxparams": 8,
  "maxdepth": 4,
  "maxlen": false,

  "asi": false,
  "boss": false,
  "debug": false,
  "eqnull": true,
  "es5": false,
  "esnext": false,
  "evil": false,
  "expr": false,
  "funcscope": false,
  "globalstrict": false,
  "iterator": false,
  "lastsemic": false,
  "laxbreak": false,
  "laxcomma": false,
  "loopfunc": true,
  "multistr": false,
  "onecase": false,
  "proto": false,
  "regexdash": false,
  "scripturl": false,
  "smarttabs": false,
  "shadow": false,
  "sub": false,
  "supernew": false,
  "validthis": false,

  "browser": false,
  "devel": false,
  "node": true,

  "globals": {
    "atom": false,
    "document": false
  }
}
